<%@ page language="java" contentType="text/html; charset=utf-8"	pageEncoding="ISO-8859-1" import="model.User" import="model.Graphs"	import="java.util.List"%><%	User u = (User) request.getSession().getAttribute("LOGADO");			List<Graphs> pizza = (List<Graphs>) request.getSession().getAttribute("pizza");			List<Graphs> notas = (List<Graphs>) request.getSession().getAttribute("notas");%><div class="container">	<ul class="nav nav-tabs" id="myTab" role="tablist">		<li class="nav-item"><a class="nav-link active" id="home-tab"			data-toggle="tab" href="#home" role="tab" aria-controls="home"			aria-selected="true">Dashboard</a></li>		<li class="nav-item"><a class="nav-link" id="profile-tab"			data-toggle="tab" href="#dados" role="tab" aria-controls="dados"			aria-selected="false">Tabelas</a></li>		<li class="nav-item"><a class="nav-link" id="contact-tab"			data-toggle="tab" href="#contato" role="tab" aria-controls="contact"			aria-selected="false">Coordenadores</a></li>	</ul>	<div class="tab-content jumbotron" id="myTabContent">		<div class="tab-pane fade show active" id="home" role="tabpanel"			aria-labelledby="home-tab">			<div class="row">				<div class="col-lg-6 col-md-12 col-sm-12">					<div class="card border-dark">						<div class="card-header">Relação de Chamados</div>						<div class="card-body">							<div id="relacao"></div>						</div>					</div>				</div>				<div class="col-lg-6 col-md-12 col-sm-12">					<div class="card border-dark">						<div class="card-header">Aprovação dos serviços</div>						<div class="card-body">							<div id="aprovacao"></div>						</div>					</div>				</div>			</div>		</div>		<div class="tab-pane fade" id="dados" role="tabpanel" aria-labelledby="dados">			<table class="table table-hover table-striped">				 <thead>				 	<th>				 		<td>#</td>				 		<td>Titulo</td>				 		<td>Cliente</td>				 		<td>Data Abertura</td>				 	</th>				 </thead>			</table>		</div>		<div class="tab-pane fade" id="contato" role="tabpanel" aria-labelledby="contact-tab">...</div>	</div></div><script type="text/javascript">google.charts.load('current', { 'packages' : [ 'corechart' ]});google.charts.setOnLoadCallback(drawChart);function drawChart() {	var options_pizza = {		//'title' : 'Relação de requisições',		'width'  : 420,		'height' : 370	};	var options_lines = {			'width'  : 370,			'height' : 370,			curveType: 'function',			legend: { position: 'bottom' }	};	var data = new google.visualization.DataTable();	data.addColumn('string', 'Descrição');	data.addColumn('number', 'Quantidade');	data.addRows([<%for (Graphs p : pizza) {				if (pizza.indexOf(p) != (pizza.size() - 1)) {%>		[ '<%=p.getValor()%>', <%=p.getQtd()%> ], <%} else {%>		[ '<%=p.getValor()%>', <%=p.getQtd()%> ] <%}%><%}%>		]);	var chart = new google.visualization.PieChart(document.getElementById('relacao'));	chart.draw(data, options_pizza);	var lines = google.visualization.arrayToDataTable([				[ 'Pontuação', 'Notas' ], <%for (Graphs n : notas) {				if (notas.indexOf(n) != (notas.size() - 1)) {%>				[ '<%=n.getValor()%>', <%=n.getQtd()%> ], <%} else {%>				[ '<%=n.getValor()%>', <%=n.getQtd()%> ] <%}%><%}%>		    	]);    var arr = new google.visualization.LineChart(document.getElementById('aprovacao'));    arr.draw(lines, options_lines);}</script>