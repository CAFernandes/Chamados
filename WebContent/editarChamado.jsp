<%@ page language="java" contentType="text/html; charset=ISO-8859-1"	pageEncoding="ISO-8859-1" import="model.Requisicao" import="model.User"%><%	User u = (User) request.getSession().getAttribute("LOGADO");	Requisicao c = (Requisicao) request.getSession().getAttribute("EDIT");%><!DOCTYPE html> <html><head>	<jsp:include page="includes/head.jsp"></jsp:include></head><body>	<jsp:include page="includes/menu.jsp"></jsp:include>	<header class="container-fluid jumbotron" id="menu">		<hgroup>			<h2>Edit Calls</h2>		</hgroup>	</header>	<div class="container-fluid table-responsive" id="formulario">		<form action="./editarChamado" class="form-horizontal" method="POST">			<% if (String.valueOf(u.getPermissao()).equals("T")) { %>			<div class="form-group">				<label class="col-sm-2 control-label">Id:</label>				<div class="col-sm-10">					<input class="form-control" type="text" readonly name="id" value="<%=c.getId()%>">				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Data Abertura:</label>				<div class="col-sm-10">					<input class="form-control" type="text" readonly name="data_abertura" 					value="<%=String.valueOf(c.getData_abertura())%>">				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Titulo:</label>				<div class="col-sm-10">					<input class="form-control" type="text" readonly name="titulo"						value="<%=c.getTitulo()%>">				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Conteudo:</label>				<div class="col-sm-10">					<textarea class="form-control" readonly name="conteudo"><%=c.getDescricao()%> </textarea>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Situação:</label>				<div class="col-sm-10">					<input class="form-control" list="estado" name="situacao" autofocus required>					<datalist id="estado">					<option value="Atendido">					<option value="Em Análise">					<option value="Em Espera">					</datalist>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Resposta:</label>				<div class="col-sm-10">					<textarea class="form-control" name="resposta"> </textarea>				</div>			</div>			<div class="form-group">				<div class="col-sm-offset-2 col-sm-10">					<button type="submit" class="btn btn-primary btn-block">Responder</button>				</div>			</div>			<%} else{ %>			<div class="form-group">				<label class="col-sm-2 control-label">Id:</label>				<div class="col-sm-10">					<input class="form-control" type="text" readonly name="id"						value="<%=c.getId()%>">				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Data Abertura:</label>				<div class="col-sm-10">					<input class="form-control" type="text" readonly name="data_abertura" value="<%=String.valueOf(c.getData_abertura())%>">				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Titulo:</label>				<div class="col-sm-10">					<input class="form-control" type="text" name="titulo" value="<%=c.getTitulo()%>" autofocus>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Conteudo:</label>				<div class="col-sm-10">					<textarea class="form-control" name="conteudo"><%=c.getDescricao()%> </textarea>				</div>			</div>			<div class="form-group">				<div class="col-sm-offset-2 col-sm-10">					<Button type="submit" class="btn btn-primary btn-block">Alterar</Button>				</div>			</div>			<%				}			%>		</form>	</div>	<jsp:include page="includes/footer.jsp"></jsp:include></body></html>