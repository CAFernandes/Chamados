<%@ page language="java" contentType="text/html; charset=ISO-8859-1"	pageEncoding="ISO-8859-1" import="model.User"%><%	User u = (User) request.getSession().getAttribute("LOGADO");	User edit = (User) request.getSession().getAttribute("EDIT");%><!DOCTYPE html><html><head>	<jsp:include page="includes/head.jsp"></jsp:include></head><body>	<jsp:include page="includes/menu.jsp"></jsp:include>	<div class="container-fluid jumbotron" id="menu">		<h2>Edit User</h2>	</div>	<div class="container-fluid table-responsive" id="formulario">		<form action="./editarUser" class="form-horizontal" method="POST">			<div class="form-group">				<label class="col-sm-2 control-label">Id:</label>				<div class="col-sm-10">					<input class="form-control" type="text" name="id" value="<%=edit.getId()%>" readonly>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Nome:</label>				<div class="col-sm-10">					<input class="form-control" type="text" name="nome" value="<%=edit.getNome()%>" required>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">E-mail:</label>				<div class="col-sm-10">					<input class="form-control" type="text" name="email" value="<%=edit.getEmail()%>" required>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Senha:</label>				<div class="col-sm-10">					<input class="form-control" type="password" name="senha" value="<%=edit.getSenha()%>" required>				</div>			</div>			<div class="form-group">				<label class="col-sm-2 control-label">Administrador:</label>				<div class="col-sm-10">					<input class="form-control" type="text" name="level" value="<%=edit.getPermissao() %>" readonly >				</div>			</div>			<div class="form-group">				<div class="col-sm-offset-2 col-sm-10">					<button type="submit" class="btn btn-primary btn-block">Alterar</button>				</div>			</div>		</form>	</div>	<jsp:include page="includes/footer.jsp"></jsp:include></body></html>